{
    "name": "umulmrum/holiday",
    "description": "A PHP library to compute holidays. It's something :-)",
    "keywords": [
        "holiday",
        "holidays",
        "calendar",
        "date",
        "time"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Kruppa",
            "email": "stefan@umulmrum.de"
        }
    ],
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "chameleon-system/code-style-config": "dev-master@dev",
        "phpunit/phpunit": "~9.4",
        "phpspec/prophecy-phpunit": "^2.0",
        "symfony/translation": "~4.3|~5.0"
    },
    "autoload": {
        "psr-4": {
            "umulmrum\\Holiday\\": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "umulmrum\\Holiday\\Test\\": ["tests"]
        }
    }
}
