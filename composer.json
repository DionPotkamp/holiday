{
    "name": "umulmrum/holiday",
    "description": "A PHP library to compute holidays. It's something :-)",
    "keywords": [
      "holiday",
      "holidays",
      "calendar",
      "date",
      "time"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Kruppa",
            "email": "stefan@umulmrum.de"
        }
    ],
    "require": {
        "ext-calendar": "*",
        "php": "~5.4|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4,>5.4.2|~6.0",
        "symfony/translation": "~2.0,>2.0.4|~3.0|~4.0"
    },
    "autoload": {
      "psr-4": {
        "umulmrum\\Holiday\\": ["src"]
      },
      "exclude-from-classmap": ["/tests/"]

},
    "autoload-dev": {
      "psr-4": {
        "umulmrum\\Holiday\\": ["tests"]
      }
    }
}
